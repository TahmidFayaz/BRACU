class GS_Generator:

    def __init__(self, student_id, target):

        self.student_id = student_id
        self.target = target
        self.weights = []
        self.pool = []

    def get_weights(self, Target_Length):

        ID = str(self.student_id)
        last_n_digits = ID[-Target_Length:]

        weights = []
        for i in last_n_digits:
            weights.append(int(i))

        return weights

    def Booster(self):

        ID = str(self.student_id)
        FD_2 = int(ID[:2])

        return FD_2 / 100.0

    def Booster_Application(self, weights, S):

        multiplier = self.Booster()
        modified_weights = weights[:]


        # from S position
        i = S
        while i < len(modified_weights):
            modified_weights[i] = modified_weights[i] * multiplier
            i += 1

        return modified_weights

    def Utility_Calculation(self, gene_sequence, target, weights, GB=False, S=-1):

        # Apply genetic booster if active
        GBW = weights
        if GB and S >= 0:
            GBW = self.Booster_Application(weights, S)

        utility = 0
        p = max(len(gene_sequence), len(target))


        for i in range(p):
            if i < len(GBW):
                weight = GBW[i]
            else:
                weight = 1

            #Gene
            if i < len(gene_sequence):
                gene_char = gene_sequence[i]
                Gene_ASCII = ord(gene_char)
            else:
                Gene_ASCII = 0

            #Target
            if i < len(target):
                target_char = target[i]
                Target_ASCII = ord(target_char)
            else:
                Target_ASCII = 0


            d = abs(Gene_ASCII - Target_ASCII)


            utility = utility - (weight * d)


        return utility

    def minimax(self, current_sequence, remaining_pool, depth, maximizing_player, alpha, beta,GB_Status=False, S=-1):



        if depth == 0 or not remaining_pool:
            utility = self.Utility_Calculation(current_sequence, self.target, self.weights,GB_Status, S)
            return utility, current_sequence

        if maximizing_player:
            max_eval = float('-inf')
            best_sequence = current_sequence

            i = 0
            while i < len(remaining_pool):
                nucleotide = remaining_pool[i]

                new_sequence = current_sequence + nucleotide
                new_pool = remaining_pool[:i] + remaining_pool[i+1:]

                New_Booster =GB_Status
                new_S = S
                if nucleotide == 'S' and not GB_Status:
                    New_Booster = True
                    new_S = len(current_sequence)

                eval_score, sequence = self.minimax(new_sequence, new_pool, depth - 1, False, alpha, beta, New_Booster, new_S)

                if eval_score > max_eval:
                    max_eval = eval_score
                    best_sequence = sequence

                # Alpha-beta pruning
                alpha = max(alpha, eval_score)
                if beta <= alpha:
                    break

                i += 1

            return max_eval, best_sequence

        else:
            min_eval = float('inf')
            best_sequence = current_sequence

            i = 0
            while i < len(remaining_pool):
                nucleotide = remaining_pool[i]


                new_sequence = current_sequence + nucleotide
                new_pool = remaining_pool[:i] + remaining_pool[i+1:]

                New_Booster =GB_Status
                new_S = S


                eval_score, sequence = self.minimax(new_sequence, new_pool, depth - 1, True, alpha, beta, New_Booster, new_S)

                if eval_score < min_eval:
                    min_eval = eval_score
                    best_sequence = sequence

                # Alpha-beta pruning
                beta = min(beta, eval_score)
                if beta <= alpha:
                    break

                i += 1

            return min_eval, best_sequence

    def generate_gene_sequence(self, Special_Nucleotide=False):


        if Special_Nucleotide:
            pool = ['A', 'T', 'C', 'G', 'S']  # S at last
        else:
            pool = ['A', 'T', 'C', 'G']


        self.pool = []
        i = 0
        while i < len(pool):
            self.pool.append(pool[i])
            i += 1


        self.weights = self.get_weights(len(self.target))


        max_depth = len(pool)
        best_score, best_sequence = self.minimax("", pool, max_depth, True, float('-inf'), float('inf'))

        return best_sequence, best_score

    def Scenarios(self):

        sequence_without, score_without = self.generate_gene_sequence(Special_Nucleotide=False)

        sequence_with, score_with = self.generate_gene_sequence(Special_Nucleotide=True)

        use_special = score_with > score_without

        results = {}


        results['without_special'] = {'sequence': sequence_without, 'score': score_without}
        results['with_special'] = {'sequence': sequence_with, 'score': score_with}

        if use_special:
            results['recommendation'] = 'YES'
        else:
            results['recommendation'] = 'NO'


        if use_special:
            Decision  = f"YES, because without special nucleotide Score: {score_without} but With special nucleotide Score: {score_with}"
        else:
            Decision  = f"NO, because without special nucleotide Score: {score_without} but With special nucleotide Score: {score_with}"

        results['explanation'] = Decision

        return results

    def Info(self):

        game_info = {}
        game_info['student_id'] = self.student_id
        game_info['pool'] = self.pool
        game_info['weights'] = self.weights
        game_info['GBM'] = self.Booster()
        game_info['target'] = self.target

        return game_info


    def Print(self):

        result = self.Scenarios()

        print(f"{result['recommendation']}")
        print("With special nucleotide")
        print(f"Best gene sequence generated: {result['with_special']['sequence']},")
        print(f"Utility score: {result['with_special']['score']}")
        print(f"{result['explanation']}\n")




