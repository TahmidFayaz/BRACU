class GS_Generator:

    def __init__(self, student_id, target):
        self.student_id = student_id
        self.target = target
        self.pool = []
        self.weights = []

    def get_weights(self, Target_Length):

        ID = str(self.student_id)
        ID_Length= len(ID)


        if Target_Length <= ID_Length :
            last_n_digits = ID[-Target_Length:]
        else:
            last_n_digits = ID

        while  Target_Length > len(last_n_digits):
            last_n_digits = '0' + last_n_digits

        weights = []
        for i in last_n_digits:
            weights.append(int(i))

        return weights

    def Utility_Calculation(self, gene_sequence, target, weights):

        p= max(len(gene_sequence), len(target))
        utility = 0

        i = 0
        while i < p:

            if i < len(weights):
                weight = weights[i]
            else:
                weight = 1

            #Gene
            if i < len(gene_sequence):
                Gene_ASCII = ord(gene_sequence[i])
            else:
                Gene_ASCII = 0

            #Target
            if i < len(target):
                Target_ASCII = ord(target[i])
            else:
                Target_ASCII = 0

            d = abs(Gene_ASCII - Target_ASCII)
            utility -= weight * d

            i += 1

        return utility

    def minimax(self, current_sequence, remaining_pool, depth, maximizing_player, alpha, beta):



        if depth == 0 or not remaining_pool:
            utility = self.Utility_Calculation(current_sequence, self.target, self.weights)
            return utility, current_sequence

        if maximizing_player:
            max_eval = float('-inf')
            best_sequence = current_sequence

            i = 0
            while i < len(remaining_pool):
                nucleotide = remaining_pool[i]

                new_sequence = current_sequence + nucleotide
                new_pool = remaining_pool[:i] + remaining_pool[i+1:]


                eval_score, sequence = self.minimax(new_sequence, new_pool, depth - 1, False, alpha, beta)

                if eval_score > max_eval:
                    max_eval = eval_score
                    best_sequence = sequence

                # Alpha-beta pruning
                alpha = max(alpha, eval_score)
                if beta <= alpha:
                    break

                i += 1

            return max_eval, best_sequence

        else:

            min_eval = float('inf')
            best_sequence = current_sequence

            i = 0
            while i < len(remaining_pool):
                nucleotide = remaining_pool[i]

                new_sequence = current_sequence + nucleotide
                new_pool = remaining_pool[:i] + remaining_pool[i+1:]


                eval_score, sequence = self.minimax(new_sequence, new_pool, depth - 1, True, alpha, beta)

                if eval_score < min_eval:
                    min_eval = eval_score
                    best_sequence = sequence

                # Alpha-beta pruning
                beta = min(beta, eval_score)
                if beta <= alpha:
                    break

                i += 1

            return min_eval, best_sequence

    def generate_gene_sequence(self):


        pool = ['A', 'T', 'C', 'G']


        self.pool = []
        i = 0
        while i < len(pool):
            self.pool.append(pool[i])
            i += 1


        self.weights = self.get_weights(len(self.target))

        max_depth = len(pool)
        best_score, best_sequence = self.minimax("", pool, max_depth, True, float('-inf'), float('inf'))

        return best_sequence, best_score

    def Info(self):
        game_info = {}
        game_info['pool'] = self.pool
        game_info['target'] = self.target
        game_info['student_id'] = self.student_id
        game_info['weights'] = self.weights
        return game_info


